{
  "name": "iwanttorage",
  "description": "Life is hard. To relieve stress, I must destroy.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "rage_guy.png"
  } ,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["libs/matter.min.js", "dist/content.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ]
}